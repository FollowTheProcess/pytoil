[flake8]
exclude =
	.git,
	__pycache__,
	.pytest_cache,
	.mypy_cache,
	venv/,
	.venv/,
	docs,
	site,
	.nox,
	*.egg,
	*.egg-info,
	build,
	dist
max-complexity = 15
max-line-length = 120
extend-ignore =
	# Black compatibility: whitespace before :
	E203,
	# Black compatibility: line break before binary operator
	W503,
extend-select = B9
